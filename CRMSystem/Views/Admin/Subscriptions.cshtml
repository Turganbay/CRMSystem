<div>
    <div class="divList">
        <table class="table table-hover">
            <tr>
                <td><b>ID</b></td>
                <td><b>EVENT ID</b></td>
                <td><b>EVENT NAME</b></td>
                <td><b>USER ID</b></td>
                <td><b>USER NAME</b></td>
                <td><b>ACTION</b></td>
            </tr>
            <tr ng-repeat="sub in subscriptions">
                <td>{{sub.id}}</td>
                <td>{{sub.event_id}}</td>
                <td>{{sub.event_name}}</td>
                <td>{{sub.user_id}}</td>
                <td>{{sub.user_name}}</td>
                <td>
                    <span ng-click="deleteSubscription(sub)" class="btn btn-danger">Delete</span>
                </td>
            </tr>
        </table>
    </div>
</div>
